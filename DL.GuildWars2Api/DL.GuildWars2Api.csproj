<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Digital Lagniappe</Authors>
    <Company>Digital Lagniappe</Company>
    <Description>C# wrapper for the GW2 public API</Description>
    <Copyright>Digital Lagniappe</Copyright>
    <PackageLicenseUrl>https://github.com/digital-lagniappe/guildwars2api/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/digital-lagniappe/guildwars2api</PackageProjectUrl>
    <PackageReleaseNotes>initial release</PackageReleaseNotes>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.Net.Http" Version="4.3.2" />
  </ItemGroup>

</Project>
